<../../mkconfig

TARG=0c

OFILES=	cgen.$O\
	enam.$O\
	list.$O\
	peep.$O\
	reg.$O\
	sgen.$O\
	swt.$O\
	txt.$O\
	mul.$O\

HFILES=	gc.h\
	v.out.h\
	../cc/cc.h\

LIBS=cc bio 9		# order is important

BIN=$SRC/$OBJDIR/bin

<$SRC/mkfiles/mkone-$SHELLTYPE

CFLAGS=	$CFLAGS -I../include

$SRC/$OBJDIR/lib/libcc.a:
	cd ../cc
	mk $MKFLAGS install
	mk $MKFLAGS clean
